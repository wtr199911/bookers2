<h1><strong>User info</strong></h1>
<%= form_with model: @edituser do |f| %>

  <label for="inputName">Name</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%>
  <label for="inputImage">image</label>
  <%= f.file_field :image, placeholder: "プロフィール画像", accept: "image/*" %>
  <label for="inputImage">introduction</label>
  <%= f.text_area :introduction %>
  <%= f.submit "Update User" %>
<% end %>